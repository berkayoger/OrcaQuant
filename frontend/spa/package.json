{
  "name": "orcaquant-spa",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "axios": "^1.11.0",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "classnames": "^2.5.1",
    "lucide-react": "^0.525.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.4",
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^5.4.8",
    "tailwindcss": "^3.4.13",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "concurrently": "^9.0.1"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "dev:full": "concurrently \"vite --host 0.0.0.0\" \"cd ../../backend && FLASK_APP=backend.app:create_app flask run --reload\"",
    "build": "vite build",
    "build:copy": "node -e \"const path=require('path');const fs=require('fs');const src=path.resolve('dist');if(!fs.existsSync(src)){console.error('dist klasörü bulunamadı');process.exit(1);}const dest=path.resolve('../../backend/static');fs.rmSync(dest,{recursive:true,force:true});fs.mkdirSync(dest,{recursive:true});fs.cpSync(src,dest,{recursive:true});\"",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
